{
  "name": "flowscript-syntax",
  "displayName": "FlowScript Syntax",
  "description": "Syntax highlighting for the FlowScript language (simple example TextMate grammar).",
  "version": "0.0.1",
  "publisher": "simon",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "flowscript",
        "aliases": ["FlowScript", "flowscript"],
        "extensions": [".flow"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "flowscript",
        "scopeName": "source.flowscript",
        "path": "./syntaxes/flowscript.tmLanguage.json"
      }
    ]
  }
}
